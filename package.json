{
  "name": "@paunovic/utils",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@9.4.0",
  "description": "A versatile collection of utility functions for simplifying JavaScript development.",
  "author": "Milo≈° Paunovic <paun992@hotmail.com>",
  "license": "MIT",
  "homepage": "https://milospaunovic.github.io/utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilosPaunovic/utils.git"
  },
  "bugs": "https://github.com/MilosPaunovic/utils/issues",
  "keywords": [
    "utils",
    "helpers"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish --access=public",
    "start": "esno src/index.ts",
    "test": "vitest run && playwright test",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.10",
    "@playwright/test": "^1.45.0",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.4.0",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vite": "^5.3.2",
    "vitepress": "^1.2.3",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
